{
  "name": "sigap-malut-backend",
  "version": "1.0.0",
  "description": "Backend API untuk SIGAP Malut - Sistem Informasi Terintegrasi Dinas Pangan Maluku Utara",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "generate:schema": "node scripts/generateSchema.js",
    "generate:models": "node scripts/generateModels.js",
    "generate:controllers": "node scripts/generateControllers.js",
    "generate:routes": "node scripts/generateRoutes.js",
    "generate:route-index": "node scripts/generateRouteIndex.js",
    "generate:all": "npm run generate:schema && npm run generate:models && npm run generate:controllers && npm run generate:routes && npm run generate:route-index",
    "seed": "node database/seeders/seed.js",
    "migrate": "node database/migrations/migrate.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.0",
    "sqlite3": "^5.1.6",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "csv-parser": "^3.0.0",
    "exceljs": "^4.4.0",
    "pdfkit": "^0.13.0",
    "openai": "^4.20.0",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
